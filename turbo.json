{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["NEXT_PUBLIC_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY", "NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_ENV", "BEE_HIIV_TOKEN", "BEE_HIIV_ID", "BIRDEYE_KEY", "LOOPS_KEY", "NEXT_PUBLIC_VAULT_KEY", "SUPABASE_SERVICE_KEY", "DISCORD_CLIENT_ID", "DISCORD_CLIENT_SECRET", "MANAGER_KEYPAIR", "GIVEAWAY_PASSWORD", "SOLANA_RPC_URL", "SOLANA_WSS_URL", "LISTING_PASSWORD"],
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
